    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Registrer deg</title>
    <link rel="stylesheet" href="/css/main.css">
</head>
<body>
    <nav>
        <a href="/">Logo</a>
        <a href="/guide">Guide</a>
        <a href="/login">Logg inn</a>
    </nav>

    <form action="/signinn" method="post">
        <h2>Email</h2>
        <input type="email" placeholder="Skriv inn e-post" name="email" id="email" required>
        <h2>Passord</h2>
        <input type="password" placeholder="Skriv inn passord" name="password" id="password" required>
        <h2>Gjenta passord</h2>
        <input type="password" placeholder="Gjenta passord" name="password2" id="password2" required>
        <input type="submit" value="Registrer deg">
    </form>
    <a href="/login">Logg inn</a>
</body>
</html>
